# alias 清单

alias zshconfig="mate ~/.zshrc"
alias ohmyzsh="mate ~/.oh-my-zsh"
alias l="ls | lolcat"
alias ll="ls -al"
alias la="ls -A | lolcat"
alias lc="ls -CF | lolcat"
alias vi="vim"
alias copy="cp -i"
alias rm="rm -i"
alias python="python3"
alias grep="grep --color=auto"
alias ..="cd .."
alias wget="wget -c"
alias ping="ping -c 4 "
alias tree="tree -N"
alias unix13="python3 ~/Script/unix13.py | lolcat"
alias uuid-32="python3 ~/Script/uuid-32.py | lolcat"
alias uuid32="python3 ~/Script/uuid32.py | lolcat"
alias r8="python3 ~/Script/r8.py | lolcat"
alias flag="python3 ~/Script/flag.py"
alias rmd="python3 ~/Script/rmd.py"
alias mkmd="python3 ~/Script/mkmd.py "
alias vscode="/Applications/Visual\ Studio\ Code.app/Contents/Resources/app/bin/code"
alias code="/Applications/Visual\ Studio\ Code.app/Contents/Resources/app/bin/code"
alias npc="nohup /Applications/npc/npc -config=/Applications/npc/conf/npc.conf &"
alias msf="/opt/metasploit-framework/bin/msfconsole"
# brew install trash
alias rm="trash"
alias py3="source ~/Script/py39/bin/activate"
alias qpy="deactivate"
alias py2="source ~/Script/py27/bin/activate"
#alias py_env="echo 'python3 -m venv <venv-name>'"
#alias py_act="echo 'source <venv-name>/bin/activate'"
alias now="date +%Y-%m-%d\ %H:%M:%S | lolcat"
alias now_cn="date +%Y年%m月%d日 | lolcat"
alias now_s="date +%Y%m%d%H%M%S | pbcopy"
alias htop="sudo htop"
alias wx="open -n /Applications/WeChat.app/Contents/MacOS/WeChat"
alias httpserver="python3 -m http.server "
alias sha256="openssl dgst -sha256"
alias sha1="openssl dgst -sha1"
alias yg="python3 ~/Script/yg.py"
alias code_des="python3 ~/Script/code_des.py"
alias x="exit"
alias fmd="python3 ~/Script/format_markdown.py"
alias mdir="python3 ~/Script/mdir.py"
alias ipp="osascript -e 'IPv4 address of (system info)'"

# hacktools
alias hacktools="tree -L 2 ~/HackTools"
alias masscan="~/HackTools/Information/masscan/bin/masscan -h"
alias oneforall="python3 ~/HackTools/Information/OneForAll/oneforall.py "
alias exploit-db="open 'https://www.exploit-db.com'"
alias fofa="open 'https://fofa.info'"
alias nessus="~/HackTools/VulnScan/nessus"
alias awvs="~/HackTools/VulnScan/awvs"

